<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lvl1">

    <ImageView
        android:id="@+id/imageViewGarder"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:srcCompat="@drawable/fon1" />

    <ImageView
        android:id="@+id/imageViewZombieW"
        android:layout_width="195dp"
        android:layout_height="201dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.59000003"
        app:srcCompat="@drawable/zombie" />

    <ImageView
        android:id="@+id/imageViewRoom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fon2" />

    <ImageView
        android:id="@+id/imageViewWindow"
        android:layout_width="173dp"
        android:layout_height="172dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.41000003"
        app:srcCompat="@drawable/window" />

    <ImageView
        android:id="@+id/imageViewDoor"
        android:layout_width="143dp"
        android:layout_height="212dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:srcCompat="@drawable/door" />

    <ImageView
        android:id="@+id/imageViewHikkiB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.92"
        app:srcCompat="@drawable/hero4" />

    <ImageView
        android:id="@+id/imageViewBad"
        android:layout_width="255dp"
        android:layout_height="433dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="492dp"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:srcCompat="@drawable/bad" />

    <ImageView
        android:id="@+id/imageViewHikkiR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/hero1" />

    <ImageView
        android:id="@+id/imageViewZombieD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.79"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.95"
        app:srcCompat="@drawable/zombie" />

    <ImageView
        android:id="@+id/imageViewHikkiD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/imageViewRoom"
        app:layout_constraintEnd_toEndOf="@id/imageViewRoom"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="@id/imageViewRoom"
        app:layout_constraintTop_toTopOf="@id/imageViewRoom"
        app:layout_constraintVertical_bias="0.95"
        app:srcCompat="@drawable/hero5" />

</android.support.constraint.ConstraintLayout>